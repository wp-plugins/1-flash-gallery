<?xml version="1.0" encoding="utf-8"?>
<gallery>
  <params>
        <group name="gallery" title="Settings">
          <p name="source" title="Images source" control="select" default="local" values="local,flickr,picasa,photobucket" />
        </group>
        
	<group name="slideshow" title="Slideshow">
	  <p name="autostart" title="Auto Start" control="checkbox"  default="0" values="1" />
	  <p name="delay" title="Delay, sec." control="slider"  default="3" values="0..20:1"/>
	  <p name="sendID" title="Show text under slideshow" control="checkbox"  default="1" values="1" />
	  <p name="pauseMusic" title="Pause Music" control="checkbox"  default="1" values="1" />
	  <p name="stopByClick" title="Stop By Click" control="checkbox"  default="1" values="1" />
          <p name="musicPath" title="Music" control="text" default="" values="" type="file" />
	</group>
	
	<group name="background" title="Background">
	  <p name="type" title="Type" control="select" default="color" values="color,image" />
	  <p name="alpha" title="Background Opacity" control="slider" default="0.7" values="0..1:0.1" />
	  <p name="color" title="Background Color" control="color" defualt="0xffffff" />
          <p name="src" title="Background Image" control="text" default="back.jpg" values="" type="file" />
	</group>
	
	<group name="navigation" title="Controls">
	  <p name="align" title="Align" control="select" default="middle" values="top,middle,bottom" />
	  <p name="hidePlayButton" title="Hide Play Button" control="checkbox" default="1" values="1" />
	  <p name="hideNavigateButtons" title="Hide Navigate Buttons" control="checkbox" default="1" values="1" />
	</group>
	
	<group name="preview" title="Preview">
	  <p name="width" title="Width" control="text" default="110" class="numeric" />
	  <p name="height" title="Height" control="text" default="73" class="numeric" />
	  <p name="alpha" title="Background Opacity" control="slider" default="0.7" values="0..1:0.1" />
	  <p name="cornerRadius" title="Corner Radius" control="text" default="15" values="" />
	  <p name="transitionEffect" title="Transition Effect" control="select" default="alpha" values="zoomOut,blur,saturation,ribble,wooble,alpha,lightening,shift,none"/>
	  <p name="transitionDuration" title="Transition Duration" control="slider" default="2" values="0..5:0.2"/>
	  <p name="shadow" title="Shadow" control="checkbox"  default="1" values="1" />
	  <p name="shadowColor" title="Shadow Color" control="color"  default="0xffffff" />
	  <p name="shadowAlpha" title="Shadow Opacity" control="slider"  default="0.7" values="0..1:0.1" />
	  <p name="shadowBlur" title="Shadow Blur" control="slider"  default="4" values="0..10:1" />
	  <p name="shadowDistance" title="Shadow Distance" control="text"  default="2" values="" class="numeric" />
	  <p name="borderWidth" title="Scroller Border Width" control="slider" default="2" values="0..10:1"/>
	  <p name="borderColor" title="Scroller Border Color" control="color" default="0xffffff"/>
	</group>
	
	<group name="image" title="Image">
	  <p name="transitionEffect" title="Transition Effect" control="select" default="lightening" values="zoomOut,blur,saturation,ribble,wooble,alpha,lightening,shift,none"/>
	  <p name="transitionDuration" title="Transition Duration" control="slider" default="0.6" values="0..5:0.2"/>
	  <p name="scaleMode" title="Scale Mode" control="select" default="noscale" values="fit, fill, noscale"/>
	  <p name="cornerRadius" title="Corner Radius" control="text" default="30" values="" />
	  <p name="changeByClick" title="Change By Click" control="checkbox"  default="1" values="1" />
	  <p name="shadow" title="Shadow" control="checkbox"  default="1" values="1" />
	  <p name="shadowColor" title="Shadow Color" control="color"  default="0x000000" />
	  <p name="shadowAlpha" title="Shadow Opacity" control="slider"  default="0.7" values="0..1:0.1" />
	  <p name="shadowBlur" title="Shadow Blur" control="slider"  default="20" values="0..10:1" />
	  <p name="shadowDistance" title="Shadow Distance" control="text"  default="1" values="" class="numeric" />
	  <p name="imageAsLink" title="Image As Link" control="checkbox"  default="0" values="1" />
	  <p name="linkTarget" title="Link Target" control="select"  default="_blank" values="_blank,_self" />
	</group>
	
	<group name="caption" title="Caption">
	  <p name="align" title="Align" control="select" default="bottom" values="top,bottom"/>
	  <p name="enable" title="Show Caption" control="checkbox" default="1" values="1"/>
	  <p name="fontSize" title="Font Size" control="slider" default="14" values="8..30:1"/>
	  <p name="fontColor" title="Font Color" control="color" default="0x196b94"/>
	  <p name="backgroundColor" title="Background Color" control="color" default="0xffffff"/>
	  <p name="backgroundAlpha" title="Background Opacity" control="slider" default="0.7" values="0..1:0.1"/>
	  <p name="visible" title="Visible" control="select" default="always" values="always,never,onHover" />
	</group>
	
	<group name="scroller" title="Scroller">
	  <p name="align" title="Align" control="select" default="top" values="top,middle,onhover" />
	  <p name="enable" title="Show Scroller" control="checkbox" default="1	" values="1"/>
	  <p name="size" title="Size" control="text" default="40" class="numeric" />
	  <p name="hideButtons" title="Hide Buttons" control="checkbox" default="1" values="1"/>
	  <p name="hidePlayButton" title="Hide Play Button" control="checkbox" default="0" values="1"/>
	  <p name="itemDistance" title="Item Distance" control="text" default="5" class="numeric" />
	  <p name="itemIndent" title="Item Indent" control="text" default="40" class="numeric" />
	  <p name="color" title="Scroller Color" control="color" default="0xffffff"/>
	  <p name="alpha" title="Scroller Opacity" control="slider" default="0" values="0..1:0.1"/>
	  <p name="borderWidth" title="Scroller Border Width" control="slider" default="0" values="0..10:1"/>
	  <p name="borderColor" title="Scroller Border Color" control="color" default="0xffffff"/>
	</group>
	
	<group name="scrollerItem" title="Scroller Item">
	  <p name="width" title="Width" control="text" default="20" class="numeric" />
	  <p name="height" title="Height" control="text" default="20" class="numeric" />
	  <p name="alpha" title="Scroller Item Opacity" control="slider" default="1" values="0..1:0.1"/>
	  <p name="shadow" title="Shadow" control="checkbox" default="0" values="1" />
	  <p name="borderColor" title="Border Color" control="color" default="0"/>
	  <p name="shadowColor" title="Shadow Color" control="color" default="0xff0000"/>
	  <p name="shadowAlpha" title="Shadow Opacity" control="slider" default="0.7" values="0..1:0.1"/>
	  <p name="shadowBlur" title="Shadow Blur" control="slider" default="4" values="0..10:1"/>
	  <p name="shadowDistance" title="Shadow Distance" control="text" default="4" values="" class="numeric" />
	  <p name="borderWidth" title="Border Width" control="slider" default="1" values="0..10:1"/>
	  <p name="textColor" title="Album Caption Text Color" control="color" default="0x000000" /> 
	  <p name="fontSize" title="Album Caption Text Size" control="slider" default="11" values="6..24:1" />
	  <p name="shapeIsRect" title="Shape is Rectangle" control="checkbox" default="1" values="1" />
	  <p name="color" title="Scroller Item Color" control="color" default="0xffffff"/>
	</group>
	
	<group name="customSkinItem" title="Skin Item">
	  <p name="backgroundActiveColor" title="Background Active Color" control="color" default="0x778899"/>
	  <p name="backgroundPassiveColor" title="Background Passive Color" control="color" default="0xffffff"/>
	  <p name="backgroundActiveAlpha" title="Active Opacity" control="slider" default="1" values="0..1:0.1"/>
	  <p name="backgroundPassiveAlpha" title="Passive Opacity" control="slider" default="0.7" values="0..1:0.1"/>
	  <p name="mainActiveColor" title="Main Active Color" control="color" default="0xffffff"/>
	  <p name="mainPassiveColor" title="Main Passive Color" control="color" default="0"/>
	  <p name="mainActiveAlpha" title="Main Active Opacity" control="slider" default="1" values="0..1:0.1"/>
	  <p name="mainPassiveAlpha" title="Main Passive Opacity" control="slider" default="1" values="0..1:0.1"/>
	  <p name="borderActiveColor" title="Border Active Color" control="color" default="0x894554"/>
	  <p name="borderPassiveColor" title="Border Passive Color" control="color" default="0"/>
	  <p name="borderActiveAlpha" title="Border Active Opacity" control="slider" default="1" values="0..1:0.1"/>
	  <p name="borderPassiveAlpha" title="Border Passive Opacity" control="slider" default="1" values="0..1:0.1"/>
	  <p name="shadow" title="Shadow" control="checkbox" default="1" values="1" />
	  <p name="shadowColor" title="Shadow Color" control="color" default="0x000033"/>
	  <p name="shadowAlpha" title="Shadow Opacity" control="slider" default="0.7" values="0..1:0.1"/>
	  <p name="shadowBlur" title="Shadow Blur" control="slider" default="4" values="0..10:1"/>
	  <p name="shadowDistance" title="Shadow Distance" control="text" default="4" values="" class="numeric" />
	</group>
	
	<group name="flickr" title="Flickr" type="source">
	  <p name="searchBy" title="Search By" control="select" default="photos" values="photos,keyword,userID,albumID" />
	  <p name="userID" title="User ID" control="text" default="" />
	  <p name="keyword" title="Keyword" control="text" default="" />
	  <p name="max" title="Max Photos Number" control="text" class="numeric" default="20" />
	  <p name="albumID" title="Album ID" control="text" class="numeric" />
	</group>
	
	<group name="picasa" title="Picasa" type="source">
	  <p name="searchBy" title="Search By" control="select" default="photos" values="photos,keyword,userID,albumID" />
	  <p name="userName" title="User Name" control="text" default="" />
	  <p name="keyword" title="Keyword" control="text" default="" />
	  <p name="max" title="Max Photos Number" control="text" class="numeric" default="20" />
	  <p name="albumID" title="Album ID" control="text" class="numeric" default="" />
	  <p name="albumName" title="Album Name" control="text" default="" />
	</group>
	
	<group name="photobucket" title="Photobucket" type="source">
	  <p name="searchString" title="Search" control="text" default="" />
	  <p name="max" title="Max Photos Number" control="text" class="numeric" default="20" />
	</group>

	</params>
</gallery>