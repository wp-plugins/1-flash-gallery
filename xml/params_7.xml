<?xml version="1.0" encoding="utf-8"?>
<gallery>
  <params>
	<group name="slideshow" title="Slideshow">
		<p name="autostart" title="Auto Start" control="checkbox" default="0" values="1" />
		<p name="delay" title="Delay" control="slider" default="2" values="1..15:1" />
		<p name="music" title="Path To Music" control="text" default="" />
	</group>
	<group name="main" title="Main">
		<p name="flipWidth" title="Flip Width" control="text" default="800" values="" />
		<p name="flipHeight" title="Flip Height" control="text" default="533" values="" />
		<p name="backgroundColor" title="Background Color" control="color" default="0xffffff"/>
		<p name="backgroundImage" title="Background Image" control="text" default="" values="" />
		<p name="backgroundImagePlacement" title="Backgroup Image Placement" control="select" default="fit" values="fit,center,top-left" />
		<p name="alwaysOpened" title="Opened" control="checkbox" default="1" values="1" />
		<p name="handOverCorner" title="Hand Over" control="checkbox" default="0" values="1" />
		<p name="dropShadowEnabled" title="Shadow" control="checkbox" default="0" values="1" />
		<p name="dropShadowHideWhenFlipping" title="Shadow Hide When Flipping" control="checkbox" default="0" values="1" />
		<p name="shadowDepth" title="Shadow Depth" control="slider" default="10" values="1..20:1" />
		<p name="flipSound" title="Sound" control="checkbox" default="0" values="1" />
		<p name="navigationBar" title="Navigation" control="checkbox" default="1" values="1" />
		<p name="navigationBarPlacement" title="Navigation Position" control="select" default="top" values="top,bottom" />
		<p name="navigationBarTitle" title="Show Navigation Title" control="checkbox" default="0" values="1" />
		<p name="navigationBarBgAlpha" title="Navigation Background Opacity" control="slider" default="0.9" values="0.1..1:0.1" />
		<p name="fitToStageOnFullscreen" title="Fit To Stage On Full Screen" control="checkbox" default="0" values="1" />
		<p name="pageAlignContent" title="Align Content" control="select" default="fit" values="fit,fill,noscale" />
		<p name="pageBackgroundColor" title="Page Background Color" control="color" default="0xffffff" />
		<p name="pageBackgroundImage" title="Page Background Image" control="text" default="" values="" />
		<p name="pageFrame" title="Frame Thickness" control="text" default="15" values="" />
		<p name="pageFrameColor" title="Frame Color" control="color" default="0x999999" />
		<p name="pageFrameAlpha" title="Frame Opacity" control="slider" default="0.5" values="0..1:0.1" />
		<p name="pageAngleProportion" title="Angle Proportion" control="text" default="10" class="numeric" />
		<p name="coverFrame" title="Frame Cover" control="checkbox" default="0" values="1" />
		<p name="coverFrameColor" title="Frame Cover Color" control="color" default="0xffffff" />
		<p name="coverFrameAlpha" title="Frame Cover Alpha" control="slider" default="0" values="0..1:0.1" />
  </group>

    <group name="menu" title="Menu">
      <p name="albumListBtn" title="Album List Button" control="checkbox" default="1" values="1" />
      <p name="exactFitBtn" title="Exact Fit Button" control="checkbox" default="1" values="1" />
      <p name="zoomInBtn" title="Zoom In Button" control="checkbox" default="1" values="1" />
      <p name="zoomOutBtn" title="Zoom Out Button" control="checkbox" default="1" values="1" />
      <p name="fitToStageBtn" title="Fit To Stage Button" control="checkbox" default="1" values="1" />
      <p name="firstBtn" title="First Button" control="checkbox" default="1" values="1" />
      <p name="previousBtn" title="Previous Button" control="checkbox" default="1" values="1" />
      <p name="navigationString" title="Navigation" control="checkbox" default="1" values="1" />
	  <p name="nextBtn" title="Next Button" control="checkbox" default="1" values="1" />
	  <p name="lastBtn" title="Last Button" control="checkbox" default="1" values="1" />
	  <p name="soundOnBtn" title="Sound On Button" control="checkbox" default="1" values="1" />
	  <p name="sounOffBtn" title="Sound Off Button" control="checkbox" default="1" values="1" />
	  <p name="printBtn" title="Print Button" control="checkbox" default="1" values="1" />
	  <p name="downloadBtn" title="Download Button" control="checkbox" default="1" values="1" />
	  <p name="fullscreenBtn" title="Full Screen Button" control="checkbox" default="1" values="1" />
	  <p name="exitFullscreenBtn" title="Exit Full Screen Button" control="checkbox" default="1" values="1" />
    </group>

  </params>
</gallery>